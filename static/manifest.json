{
  "manifest_version":2,
  "name":"chromaural visualizer",
  "version":"1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background" : {
    "scripts" : ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab"]
}